{
    "rewrites": [
        {
            "source": "/content/**",
            "destination": "/content/$1"
        }
    ],
    "redirects": [],
    "headers": [
        {
            "source": "/content/**/*.md",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/plain; charset=utf-8"
                }
            ]
        }
    ],
    "trailingSlash": false,
    "cleanUrls": false,
    "directoryListing": false
}