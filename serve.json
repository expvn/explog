{
    "rewrites": [
        {
            "source": "/content/**",
            "destination": "/content/$1"
        },
        {
            "source": "/c/**",
            "destination": "/index.html"
        },
        {
            "source": "/e/**",
            "destination": "/index.html"
        },
        {
            "source": "/s/**",
            "destination": "/index.html"
        },
        {
            "source": "/category/**",
            "destination": "/index.html"
        },
        {
            "source": "/tag/**",
            "destination": "/index.html"
        },
        {
            "source": "/posts/**",
            "destination": "/index.html"
        },
        {
            "source": "/page/**",
            "destination": "/index.html"
        }
    ],
    "redirects": [],
    "headers": [
        {
            "source": "/content/**/*.md",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/plain; charset=utf-8"
                }
            ]
        }
    ],
    "trailingSlash": false,
    "cleanUrls": false,
    "directoryListing": false
}