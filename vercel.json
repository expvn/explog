{
    "outputDirectory": ".",
    "rewrites": [
        {
            "source": "/content/pages/:type*/:path*",
            "destination": "/content/pages/:type*/:path*"
        },
        {
            "source": "/c/:page*",
            "destination": "/index.html"
        },
        {
            "source": "/e/:page*",
            "destination": "/index.html"
        },
        {
            "source": "/s/:page*",
            "destination": "/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}