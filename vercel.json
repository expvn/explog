{
    "outputDirectory": ".",
    "rewrites": [
        {
            "source": "/content/pages/:type*/:path*",
            "destination": "/content/pages/:type*/:path*"
        },
        {
            "source": "/pages/:page*",
            "destination": "/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}